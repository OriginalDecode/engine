Texture2D textures[8]		: register( t0 );
SamplerState sampler0		: register( s0 );

cbuffer debug_information	: register( b0 )
{
	int index;
	int dummy[3];
};

struct VS_OUTPUT
{
	float4 pos : SV_POSITION0;
	float2 uv : TEXCOORD;
};

float4 main(VS_OUTPUT input) : SV_Target
{
	
	return float4(textures[7].Sample(sampler0, input.uv).rgb, 1);
};
